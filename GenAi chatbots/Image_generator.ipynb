      "source": [
        "import openai\n",
        "openai.api_key = \"your-api-key\"\n",
        "\n",
        "response = openai.Image.create(\n",
        "    prompt=\"A futuristic city floating in the clouds\",\n",
        "    n=1,\n",
        "    size=\"512x512\"\n",
        ")\n",
        "\n",
        "image_url = response['data'][0]['url']\n",
        "print(\"Image URL:\", image_url)"
      ]
    }
  ]
